<!--index.wxml-->
<map
      id="1"
      style="width: 100%; height:70vh"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      covers="{{covers}}"
      show-location
    ></map>
    <view class="container">
  <!-- <view class="list">d</view> -->
<p></p>
  <button type="{{hit == false?'default':'warn'}}">{{value}}</button>
  <view wx:for="{{list}}" wx:for-item="item" wx:key="key" wx:for-index="idx" class="list">
    {{item}}
  </view>
</view>
